<?xml version="1.0"?>

<!-- CPDS with analog instruments, Dual IFR, EC135 P2 by Heiko Schulz, animation set -->

<PropertyList>


	<path>h135Dual.ac</path>
	<offsets>
		<z-m>0</z-m>
		<y-m>0.0</y-m>
		<x-m>0</x-m>

		<pitch-deg>0</pitch-deg>
		
	</offsets>
	
	<!--Instrumentation-->
	
<effect>
	<inherits-from>Effects/lightmap</inherits-from>
	<parameters>
		<texture n="1">
			<image>Aircraft/ec135/Models/AnalogPanel_LM.png</image>
		</texture>
		<condition><use>controls/lighting/instrument-lights</use></condition>
		<factor><use>systems/electrical/outputs/stdby-instrument-lights-norm</use></factor>
	</parameters>
 	<object-name>analogpanel</object-name> 
 	<object-name>Plane.066</object-name> 
</effect>	
	
	
<!-- Helionix -->
<model>
      <name>MFD0</name>
      <path>Aircraft/ec135/Models/instrumentation/EUcopter-mfd/mfd0.xml</path>
      <offsets>
	  <x-m> 0.5492</x-m>
	  <y-m> 0.25431</y-m>
	  <z-m> 0.1926</z-m>
	  <pitch-deg>-21</pitch-deg>
      </offsets>
  </model>
    
  <model>
    <name>MFD1</name>
    <path>Aircraft/ec135/Models/instrumentation/EUcopter-mfd/mfd1.xml</path>
    <offsets>
	  <x-m> 0.5492</x-m>
	  <y-m> 0.0411</y-m>
	  <z-m> 0.1926</z-m>
	  <pitch-deg>-21</pitch-deg>
    </offsets>
  </model>	
  
  <model>
    <name>MFD2</name>
    <path>Aircraft/ec135/Models/instrumentation/EUcopter-mfd/mfd2.xml</path>
    <offsets>
	  <x-m> 0.5492</x-m>
	  <y-m> -0.25262</y-m>
	  <z-m> 0.1926</z-m>
	  <pitch-deg>-21</pitch-deg>
    </offsets>
  </model>	
 
 
<!--EngineControlPanel-->
<animation>
  <type>pick</type>
  <visible>false</visible>
  <object-name>ENG1.on</object-name>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>controls/engines/engine[0]/fadec/engine-state</property>
    <value type="double">2</value>
   </binding>          
  </action>
 </animation>
 
<animation>
  <type>pick</type>
  <visible>false</visible>
  <object-name>ENG1.idle</object-name>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>controls/engines/engine[0]/fadec/engine-state</property>
    <value type="double">1</value>
   </binding>          
  </action>
 </animation>
 
<animation>
  <type>pick</type>
   <visible>false</visible>
  <object-name>ENG1.off</object-name>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>controls/engines/engine[0]/fadec/engine-state</property>
    <value type="double">0</value>
   </binding>          
  </action>
 </animation>
 
     <animation>
        <type>rotate</type>
        <object-name>eng1.swt</object-name>
    <property>controls/engines/engine[0]/fadec/engine-state</property>
        <factor>-20.0</factor>
        <center>
            <x-m>0.012</x-m>
            <y-m>-0.035</y-m>
            <z-m>0.100</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>
 
