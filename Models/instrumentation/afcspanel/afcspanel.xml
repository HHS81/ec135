<!-- animation-->
<!--CRHT Hold-->
<animation>
    <type>pick</type>
    <object-name>crht.down.cs</object-name>
        <visible>false</visible>
    <action>
        <button>0</button>
        <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>autopilot/afcs/settings/crht-ft</property>
        <step>-1</step>
        <min>3</min>
        <wrap>1</wrap>
      </binding>
    </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>crht</tooltip-id>
                <label>crht:%5.2f</label>
        <property>autopilot/afcs/settings/crht-ft</property>
            </binding>
        </hovered>
</animation>

<animation>
    <type>pick</type>
    <object-name>crht.up.cs</object-name>
        <visible>false</visible>
    <action>
        <button>0</button>
        <repeatable>true</repeatable>
    <binding>
        <command>property-adjust</command>
        <property>autopilot/afcs/settings/crht-ft</property>
        <step>1</step>
        <max>300</max>
        <wrap>1</wrap>
      </binding>
    </action>
            <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>crht</tooltip-id>
                <label>crht:%5.2f</label>
        <property>autopilot/afcs/settings/crht-ft</property>
            </binding>
        </hovered>
</animation>

<animation>
        <type>pick</type>
        <object-name>CRHTknob</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-cycle</command>
                <property>/autopilot/afcs/control/alt-mode</property>
				<value>1</value>
				<value>0</value>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>CRHT Hold</tooltip-id>
                <label>CRHT Hold: %s</label>
                <mapping>on-off</mapping>
                <property>/autopilot/afcs/control/crht</property>
            </binding>
        </hovered>
    </animation>

<animation>
        <type>rotate</type>
        <object-name>CRHTknob</object-name>
        <property>autopilot/afcs/settings/crht-ft</property>
        <factor>1</factor>
		<center>
			<x-m>0.983</x-m>
			<y-m>-0.097</y-m>
			<z-m>-0.108</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
    </animation>
	
