<animation>
	<type>select</type>
	<object-name>ALTon</object-name>
	<condition>
	<and>
	<equals>
	<property>/autopilot/afcs/locks/altitude</property>
	<value>altitude-hold</value>
	</equals>
	<not>
	<property>/autopilot/afcs/internal/altitude-armed</property>	
	</not>
	</and>
	</condition>
</animation>

