<?xml version="1.0"?>
<PropertyList>

    <path>ki229.ac</path>

    <animation>
        <type>material</type>
        <object-name>RMI.compass</object-name>       
        <object-name>VORneedle</object-name>
        <object-name>ADFneedle</object-name>    
    <emission>
          <red>0.79</red>
            <green>0.39</green>
            <blue>0.198</blue>
            <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
        </emission>
    </animation>    

    <animation>
        <type>rotate</type>
        <object-name>RMI.compass</object-name>
        <property>orientation/heading-magnetic-deg</property>
        <axis>
            <x>1.0</x>
            <y>0</y>
            <z>0.0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>VORneedle</object-name>
        <property>orientation/heading-deg</property>
        <axis>
            <x>1.0</x>
            <y>0</y>
            <z>0.0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>VORneedle</object-name>
        <property>instrumentation/nav[1]/heading-deg</property>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0.0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>ADFneedle</object-name>
        <property>instrumentation/adf/indicated-bearing-deg</property>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    
<animation>
<type>range</type>
<min-m>0</min-m>
<max-m>5</max-m>
</animation>

</PropertyList>
