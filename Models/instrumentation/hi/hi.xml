<?xml version="1.0"?>

<PropertyList>

<path>hi.ac</path>

	 <animation>
        <type>material</type>
		<object-name>PIA.lubber</object-name>
		<object-name>PIA.card</object-name>
            <emission>
          <red>0.79</red>
            <green>0.39</green>
            <blue>0.198</blue>
            <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>PIA.card</object-name>
        <property>instrumentation/magnetic-compass/indicated-heading-deg</property>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

<animation>
  <type>noshadow</type>
        <object-name>PIA.card</object-name>
</animation>

</PropertyList>