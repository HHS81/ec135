<?xml version="1.0"?>

<PropertyList>

  <name>Helionix FND master list</name>
  <w-base>450</w-base>
  <h-base>150</h-base>


  <!--  
    Column 1 (Left) of Masterlist , typ "ENG1"
  -->
  
  <layers>
      
    <layer include="masterlist-item.xml">
      <name>masterlist line 1</name>
      <params>
        <mytxt>/instrumentation/efis/fnd/masterlist[0]/msg[0]</mytxt>
        <myrgb>/instrumentation/efis/fnd/masterlist[0]/rgb</myrgb>
      </params>
      <transformations>
        <transformation><type>y-shift</type><offset>25</offset></transformation>
        <transformation><type>x-shift</type><offset>-62</offset></transformation>
      </transformations>
    </layer>

