<?xml version="1.0"?>

<PropertyList>
    <params>
      <bear>/instrumentation/nav[0]/rel-bearing-deg</bear>
      <dst>/instrumentation/nav[0]/nav-distance</dst>
      <sym>/instrumentation/efis/navd/sym[3]/sym</sym>
      <rel-course>/instrumentation/efis/navd/sym[3]/rel-course</rel-course>
    </params>
    
    <type>switch</type>
    
    <layer>
      <name>WPT symbol</name>
      <condition>
        <equals>
          <property alias="../../../../params/sym"/>
          <value>1</value>
        </equals>  
      </condition>
      <w>35</w>
      <h>35</h>
      <texture>
          <path>Aircraft/ec145/Models/instruments/EUcopter-mfd/Textures/navdisplay.rgb</path>
          <x1>    0.61      </x1>
          <x2>    0.71      </x2>
          <y1>    0.20    </y1>
          <y2>    0.30      </y2>
      </texture>
      <emissive>true</emissive>
      <transformations>
          <transformation> 
            <type>rotation</type> 
            <property alias="../../../../params/bear"/>
          </transformation> 
          <transformation>
            <type>y-shift</type> 
            <property alias="../../../../params/dst"/> 
            <scale>160</scale> 
          </transformation>
          <transformation> 
            <type>rotation</type> 
            <property alias="../../../../params/bear"/>
            <scale>-1</scale>
          </transformation> 
      </transformations>
    </layer>
    
    <layer>
      <name>VOR symbol</name>
      <condition>
        <equals>
          <property alias="../../../../params/sym"/>
          <value>0</value>
        </equals>  
      </condition>
      <w>35</w>
      <h>35</h>
      <texture>
          <path>Aircraft/ec145/Models/instruments/EUcopter-mfd/Textures/navdisplay.rgb</path>
          <x1>    0.70      </x1>
          <x2>    0.80      </x2>
          <y1>    0.20    </y1>
          <y2>    0.30      </y2>
      </texture>

      <emissive>true</emissive>
      <transformations>
          <transformation> 
            <type>rotation</type> 
            <property alias="../../../../params/bear"/>
          </transformation> 
          <transformation>
            <type>y-shift</type> 
            <property alias="../../../../params/dst"/> 
            <scale>160</scale> 
          </transformation>
          <transformation> 
            <type>rotation</type> 
            <property alias="../../../../params/bear"/>
            <scale>-1</scale>
          </transformation> 
      </transformations>
    </layer>
    
    <layer>
      <name>NDB symbol</name>
      <condition>
        <equals>
          <property alias="../../../../params/sym"/>
          <value>2</value>
        </equals>  
      </condition>
      <w>30</w>
      <h>30</h>
      <texture>
          <path>Aircraft/ec145/Models/instruments/EUcopter-mfd/Textures/navdisplay.rgb</path>
          <x1>    0.70      </x1>
          <x2>    0.80      </x2>
          <y1>    0.11    </y1>
          <y2>    0.21      </y2>
      </texture>

      <emissive>true</emissive>
      <transformations>
          <transformation> 
            <type>rotation</type> 
            <property alias="../../../../params/bear"/>
          </transformation> 
          <transformation>
            <type>y-shift</type> 
            <property alias="../../../../params/dst"/> 
            <scale>160</scale> 
          </transformation>
          <transformation> 
            <type>rotation</type> 
            <property alias="../../../../params/bear"/>
            <scale>-1</scale>
          </transformation> 
      </transformations>
    </layer>

    <layer>
      <name>airport symbol</name>
      <condition>
        <equals>
          <property alias="../../../../params/sym"/>
          <value>3</value>
        </equals>  
      </condition>
      <w>35</w>
      <h>35</h>
      <texture>
          <path>Aircraft/ec145/Models/instruments/EUcopter-mfd/Textures/navdisplay.rgb</path>
          <x1>    0.60      </x1>
          <x2>    0.70      </x2>
          <y1>    0.11    </y1>
          <y2>    0.21      </y2>
      </texture>

      <emissive>true</emissive>
      <transformations>
          <transformation> 
            <type>rotation</type> 
            <property alias="../../../../params/bear"/>
          </transformation> 
          <transformation>
            <type>y-shift</type> 
            <property alias="../../../../params/dst"/> 
            <scale>160</scale> 
          </transformation>
          <transformation> 
            <type>rotation</type> 
            <property alias="../../../../params/bear"/>
            <scale>-1</scale>
          </transformation> 
      </transformations>
    </layer>
    
    <layer>
      <name>current WPT symbol</name>
      <condition>
        <equals>
          <property alias="../../../../params/sym"/>
          <value>4</value>
        </equals>  
      </condition>
      
      <w>35</w>
      <h>35</h>
      <texture>
          <path>Aircraft/ec145/Models/instruments/EUcopter-mfd/Textures/navdisplay.rgb</path>
          <x1>    0.795      </x1>
          <x2>    0.895      </x2>
          <y1>    0.11    </y1>
          <y2>    0.21      </y2>
      </texture>
      <emissive>true</emissive>
      <transformations>
          <transformation> 
            <type>rotation</type> 
            <property alias="../../../../params/bear"/>
          </transformation> 
          <transformation>
            <type>y-shift</type> 
            <property alias="../../../../params/dst"/> 
            <scale>160</scale> 
          </transformation>
          <transformation> 
            <type>rotation</type> 
            <property alias="../../../../params/bear"/>
            <scale>-1</scale>
          </transformation> 
      </transformations>
    </layer>

    <layer>
      <name>ILS/RWY symbol</name>
      <condition>
        <equals>
          <property alias="../../../../params/sym"/>
          <value>5</value>
        </equals>  
      </condition>
      <w>30</w>
      <h>35</h>
      <texture>
          <path>Aircraft/ec145/Models/instruments/EUcopter-mfd/Textures/navdisplay.rgb</path>
          <x1>    0.61      </x1>
          <x2>    0.71      </x2>
          <y1>    0.30      </y1>
          <y2>    0.40      </y2>
      </texture>
      <emissive>true</emissive>
      <transformations>
          <transformation> 
            <type>rotation</type> 
            <property alias="../../../../params/bear"/>
          </transformation> 
          <transformation>
            <type>y-shift</type> 
            <property alias="../../../../params/dst"/> 
            <scale>160</scale> 
          </transformation>
          <transformation> 
            <type>rotation</type> 
            <property alias="../../../../params/bear"/>
            <scale>-1</scale>
          </transformation> 
          <transformation> 
            <type>rotation</type> 
            <property alias="../../../../params/rel-course"/>
          </transformation> 
          <transformation>
            <type>y-shift</type> 
            <offset>-17</offset> 
          </transformation>
      </transformations>
    </layer>

</PropertyList>