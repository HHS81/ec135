          <equals>
          <property>/instrumentation/efis/vmd/datapage</property>
          <value>0</value>
          </equals>  
     </condition>
    <emissive>true</emissive>
    <type>text</type>
    <font>arial_black</font>
    <point-size>16</point-size>
    <color>
        <red>1</red>
        <green>1</green>
        <blue>1</blue>
    </color>
    <chunks>
        <chunk>
            <type>number-value</type>
            <property>/yasim/gross-weight-lbs</property>
            <scale>0.453592</scale>
