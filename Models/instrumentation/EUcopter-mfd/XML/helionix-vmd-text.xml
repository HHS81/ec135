        </chunk>
    </chunks>
    <transformations>
        <transformation> <type>x-shift</type> <offset>-255</offset> </transformation> 
        <transformation> <type>y-shift</type> <offset>-190</offset> </transformation> 
    </transformations>
  </layer>

