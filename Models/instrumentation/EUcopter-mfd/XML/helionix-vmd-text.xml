    <chunk>
            <condition>
          <equals>
          <property>/instrumentation/efis/vmd/datapage</property>
          <value>1</value>
          </equals>  
        </condition>    
            <type>literal</type>
            <text>time zone</text>
        </chunk>
        
        <chunk>
            <condition>
          <equals>
          <property>/instrumentation/efis/vmd/datapage</property>
          <value>2</value>
          </equals>  
        </condition>    
