<?xml version="1.0"?>

<!-- Text and symbolic annunciators and background for EUcopter VMD display   
 -->

<PropertyList>
 <name>helionix-text-mask</name>
 <w-base>600</w-base>
 <h-base>800</h-base>

  <layers>
    
     <layer>
        <name>main tank fuel indicator</name>
        <w>140</w>
        <h>100</h>
        <texture>
            <path>Aircraft/ec145/Models/instruments/EUcopter-mfd/Textures/helionix-bkgr.rgb</path>
	    <x1>0.9</x1>
            <y1>0.75</y1>
            <x2>1.0</x2>
            <y2>1.0</y2>
        </texture>
	<transformations>
	    <transformation> 
		<type>x-shift</type> 
		<offset>-151</offset> 
	    </transformation> 
            <transformation>
               <type>y-shift</type>
               <offset>-145</offset>
            </transformation>
	    <transformation>
                <type>y-shift</type>
                <property>/consumables/fuel/tank[0]/level-norm</property>
                <scale>43</scale>
		<max>1</max>
		<min>0</min>
            </transformation>
        </transformations>
        <emissive>true</emissive>
    </layer>

     <layer>
        <name>LH supply tank fuel indicator</name>
        <w>40</w>
        <h>100</h>
        <texture>
            <path>Aircraft/ec145/Models/instruments/EUcopter-mfd/Textures/helionix-bkgr.rgb</path>
            <x1>0.9</x1>
            <y1>0.75</y1>
            <x2>1.0</x2>
            <y2>1.0</y2>
        </texture>
        <transformations>
            <transformation> 
                <type>x-shift</type> 
                <offset>-195</offset> 
            </transformation> 
            <transformation>
               <type>y-shift</type>
               <offset>-145</offset>
            </transformation>
            <transformation>
                <type>y-shift</type>
                <property>/consumables/fuel/tank[1]/level-norm</property>
                <scale>43</scale>
                <max>1</max>
                <min>0</min>
            </transformation>
        </transformations>
        <emissive>true</emissive>
    </layer>

     <layer>
        <name>RH supply tank fuel indicator</name>
        <w>40</w>
        <h>100</h>
        <texture>
            <path>Aircraft/ec145/Models/instruments/EUcopter-mfd/Textures/helionix-bkgr.rgb</path>
            <x1>0.9</x1>
            <y1>0.75</y1>
            <x2>1.0</x2>
            <y2>1.0</y2>
        </texture>
        <transformations>
            <transformation> 
                <type>x-shift</type> 
                <offset>-80</offset> 
            </transformation> 
            <transformation>
               <type>y-shift</type>
               <offset>-145</offset>
            </transformation>
            <transformation>
                <type>y-shift</type>
                <property>/consumables/fuel/tank[2]/level-norm</property>
                <scale>43</scale>
                <max>1</max>
                <min>0</min>
            </transformation>
        </transformations>
        <emissive>true</emissive>
    </layer>

    <layer>
    <w>600</w>
    <h>800</h>
    <name>test overlay mask for element proportions</name>
    <emissive>true</emissive>
    <texture>   
      <path>Aircraft/ec145/Models/instruments/EUcopter-mfd/Textures/helionix-overl-vmd.rgb</path>
      <x1>	0.00	</x1>
      <y1>	0.22	</y1>
      <x2>	0.58	</x2>
      <y2>	0.98	</y2>
     </texture>
  </layer>    

  <!-- note on bleed air: as cabin heating is not simulated, bleed air 
       symbols are shown by default. 
  -->
  
