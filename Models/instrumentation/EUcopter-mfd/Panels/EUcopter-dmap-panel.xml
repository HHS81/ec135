<?xml version="1.0"?>
<!DOCTYPE PropertyList SYSTEM "panel.dtd">

<!--  Digital Map (DMAP) Navigation Display Glass Cockpit Panel.

      Author: litzi

      The panel is in portrait orientation with 600x800 resolution
-->

<PropertyList>

 <name>Glass Cockpit DMAP Panel</name>
 <params>
     <ycenter>500</ycenter>
     <diameter>600</diameter>
 </params>

 <background>Aircraft/ec135/Models/instrumentation/EUcopter-mfd/Textures/black.rgb</background>
 <w>600</w> 
 <h>800</h>
  
 <instruments> 
    
 <instrument include = "../XML/terrain-map.xml">
   <condition>
       <property>instrumentation/efis/dmap/planmode</property>
   </condition>
   <x>300</x>
   <y>500</y>
   <w>600</w> 
   <h>600</h> 
 </instrument> 
  
 <instrument include = "../XML/terrain-map-mask.xml">
   <x>300</x>
   <y>500</y>
   <w>600</w> 
   <h>600</h> 
 </instrument> 
 

 <instrument include="../XML/navd-plan.xml">
        <name>overlay flight plan symbols</name>
        <condition>
            <property>/instrumentation/efis/navd/range</property>
        </condition>
        <x>300</x>
        <y alias="../../../params/ycenter"/>
        <w alias="../../../params/diameter"/>    
        <h alias="../../../params/diameter"/> 
  </instrument>

 <instrument include = "../XML/helionix-btn-hor.xml">
   <name>button labels on top</name>
    <params>
        <btn1> FND</btn1>
        <btn2>NAVD</btn2>
        <btn3> VMS</btn3>
        <btn4>DMAP</btn4>
        <type>300</type>
    </params>
   <x>220</x>
   <y>785</y>
   <w>400</w> 
   <h>20</h> 
 </instrument> 
 
  <instrument>
   <condition>
       <property>instrumentation/efis/dmap/db-mode</property>
   </condition>
    <background>Aircraft/Instruments/EUcopter-mfd/Textures/glass-black-bkg.rbg</background>
 
    <name>database literal</name>
    <w-base>600</w-base>
    <h-base>300</h-base>
    <w>600</w>
    <h>300</h>
    <x>300</x>
    <y>165</y>
    
    <layers>      
      <layer>
        <name>background</name>
        <w>600</w>
        <h>300</h>
        <emissive>true</emissive>
        <texture>
          <path>Aircraft/ec135/Models/instrumentation/EUcopter-mfd/Textures/black.rgb</path>
        </texture>
      </layer>
      
      <layer>
        <name>blue bar</name>
        <w>600</w>
        <h>38</h>
        <emissive>true</emissive>
        <texture>
          <path>Aircraft/ec135/Models/instrumentation/EUcopter-mfd/Textures/mapdisplay.rgb</path>
          <x1>0.0</x1>
          <y1>0.0</y1>
          <x2>0.45</x2>
          <y2>0.065</y2>
        </texture>
        <transformations>
        <transformation><type>y-shift</type><offset>130</offset></transformation>
        </transformations>
      </layer>
      
      <layer> 
        <point-size>18</point-size>    
        <type>text</type>
        <font>arial_black</font>
        <color>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
        </color>
        <chunks>
          <chunk>
              <type>literal</type>
              <text>DATABASE</text>
            </chunk>
        </chunks>
        <emissive>true</emissive>
        <transformations>
              <transformation><type>x-shift</type><offset>-60</offset></transformation>
              <transformation><type>y-shift</type><offset>125</offset></transformation>
        </transformations>
      </layer>      
      
      <layer> 
        <point-size>16</point-size>    
        <type>text</type>
        <font>arial_black</font>
        <color>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
        </color>
        <chunks>
          <chunk>
              <type>number-value</type>
              <format>%1.0fI3</format>
              <property>instrumentation/efis/dmap/db-page</property>
              <offset>1</offset>
            </chunk>
        </chunks>
        <emissive>true</emissive>
        <transformations>
              <transformation><type>x-shift</type><offset>240</offset></transformation>
              <transformation><type>y-shift</type><offset>125</offset></transformation>
        </transformations>
      </layer>      
    </layers>    
  </instrument>

