	<animation>
		<type>rotate</type>
		<object-name>mainrotor</object-name>
		<object-name>mainrotor_fast</object-name>
		<object-name>blurred</object-name>
<property>rotors/main/blade/position-deg</property>
		<factor>1</factor>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>mainrotor</object-name>
		<condition>
			<less-than>
