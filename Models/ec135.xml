<?xml version="1.0"?>

<!-- Eurocopter EC 135 Helicopter by Heiko Schulz, animation set -->

<PropertyList>

<nasal>
        <load>
            var livery_update = aircraft.livery_update.new(
	    "Aircraft/ec135/Models/liveries", 10,
	    func { print("ec135 livery update") });
        </load>

        <unload>
            livery_update.stop();
        </unload>
</nasal>
  
 
	<path>ec135.ac</path>
