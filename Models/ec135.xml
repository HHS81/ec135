<center>
			<x-m>0.816197</x-m>
			<y-m>-0.744383</y-m>
			<z-m>0.166909</z-m>
		</center>
		<axis>
			<x>-0.289498</x>
			<y>-0.005941</y>
			<z>-0.503452</z>
		</axis>
 </animation>
 

 
