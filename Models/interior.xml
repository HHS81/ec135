<?xml version="1.0"?>

<PropertyList>

	<path>interior.ac</path>
	<offsets>
		<z-m>0.0</z-m>
		<y-m>0.0</y-m>
		<x-m>0.0</x-m>

		<pitch-deg>0.0</pitch-deg>
		
	</offsets>
	
<effect>
		<inherits-from>Aircraft/ec135/Models/Effects/ec135reflectglas-uber</inherits-from>
		<object-name>sunshield</object-name>
</effect>
	
<model>
		<name>MagCompass</name>
		<path>Aircraft/ec135/Models/instrumentation/MagCompass/magcompass.xml</path>
		<offsets>
			<x-m>0.500</x-m>
			<y-m>0.00</y-m>
			<z-m>0.528</z-m>

			<pitch-deg>0</pitch-deg>

		</offsets>
	</model>
<!--.animations-->
<!--headset holder-->
<animation>
		<type>rotate</type>
		<object-name>headset_holder</object-name>
	<property>sim/model/ec135/door-positions/headsetHolder/position-norm</property>
	<min-deg>0</min-deg>
	<max-deg>90</max-deg>
	<factor>90</factor>
		<center>
			<x-m>0.747429</x-m>
			<y-m>-0.001443</y-m>
			<z-m>0.658929</z-m>
		</center>
		<axis>
			<x>-0.011391</x>
			<y>0</y>
			<z>0.021732</z>
		</axis>
	</animation>

<animation>
	<type>pick</type>
	<object-name>headset_holder</object-name>
	<visible>true</visible>
	<action>
		<button>0</button>
		<repeatable>false</repeatable>
		<binding>
			<command>nasal</command>
			<script>ec135.headsetHolder.toggle();</script>
		</binding>
	</action>
</animation>

<!--Doors-->
<animation>
   <type>pick</type>
   <object-name>frontdoor.L.i</object-name>
   <visible>true</visible>
   <action>
     <button>0</button>
     <repeatable>false</repeatable>
     <binding>
        <command>nasal</command>
        <script>ec135.leftFrontDoor.toggle();</script>
     </binding>
   </action>
