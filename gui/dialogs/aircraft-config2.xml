<PropertyList>

    <name>aircraft-config2</name>
    <layout>vbox</layout>
    <resizable>false</resizable>
    <modal>false</modal>
    <draggable>true</draggable>
    <group>
        <layout>hbox</layout>

        <empty><stretch>true</stretch></empty>
        <text>
            <label>Helicopter Configuration</label>
        </text>
        
        <button>
            <legend/>
            <key>Esc</key>
            <pref-width>16</pref-width>
            <pref-height>16</pref-height>
            <binding>
                <command>dialog-close</command>
            </binding>
	</button>
	</group>
	<hrule/>
    
<group>
        <default-padding>4</default-padding>
        <halign>left</halign>
        
         <text>
            <label>is set from the xml-file in Models/liveries or Models/liveries_h135p3, but can be changed here</label>
        </text>
        <empty><stretch>true</stretch></empty>            
</group>

